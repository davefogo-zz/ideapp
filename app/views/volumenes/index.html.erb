<div class="row">
  <h1>Listado de Volumenes Registrados</h1>
  <div class="pull-right range-query">
    <%= form_tag volumenes_path, method: :get do %>
        <%= text_field_tag 'search[created_at_from]', @search.created_at_from %>
        <%= text_field_tag 'search[created_at_to]', @search.created_at_to %>
      <%= submit_tag 'Buscar', class: 'btn search-button' %>
    <% end %>
  </div>
</div>
<div class="media">
  <div class="media-body">
      <table class="table table-hover">
          <thead>
            <tr>
          <th>Medio</th>
          <th>Item</th>
          <th>Tipo de Volumen</th>
          <th>Escalas</th>
          <th>Rango</th>
          <th>Cobro</th>
          <th>Valor</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @volumen.each do |volumen| %>
          <tr>
            <td><%= volumen.medio_id %></td>
            <td><%= volumen.factura_item_id %></td>
            <td><%= volumen.tipo_de_volumen %></td>
            <td><%= volumen.escala %></td>
            <td><%= volumen.rango %></td>
    		    <td><%= volumen.cobro %></td>
            <td><%= volumen.valor %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>


<br>

